{
  "private": true,
  "targets": {
    "react-server": {
      "context": "react-server",
      "source": "src/server.tsx",
      "scopeHoist": false,
      "includeNodeModules": {
        "@mjackson/node-fetch-server": false,
        "compression": false,
        "express": false
      }
    }
  },
  "postcss": {
